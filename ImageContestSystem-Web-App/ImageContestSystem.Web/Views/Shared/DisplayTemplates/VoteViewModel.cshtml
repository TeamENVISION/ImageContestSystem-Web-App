@model ImageContestSystem.Web.Models.ViewModels.VoteViewModel
<div class="col-lg-3 col-md-4 col-xs-6 thumb">
    <p>@Model.Description</p>
    @Html.ActionLink(@Model.Title, "Contest", "Vote", new {Id = @Model.ContestId}, new {@class = "thumbnail"})
    <p>@Model.AvailableVotes / @Model.VotesCount</p>
</div>