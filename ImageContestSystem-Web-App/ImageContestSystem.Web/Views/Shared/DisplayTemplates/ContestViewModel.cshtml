@model ImageContestSystem.Web.Models.ViewModels.ContestViewModel

<div class="col-lg-3 col-md-4 col-xs-6 thumb">
    @Html.ActionLink(@Model.Title, "Index", "Contest", new { Id = @Model.ContestId }, new { @class = "thumbnail"})
        <img class="img-responsive" src="@Model.PictureUrl" alt="@Model.PictureUrl">
        <p>@Model.Title</p>
</div>